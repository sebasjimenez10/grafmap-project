<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <title>Graf Map</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width">
    <link rel="stylesheet" href="assets/messenger/build/css/messenger.css">
    <link rel="stylesheet" href="assets/messenger/build/css/messenger-spinner.css">
    <link rel="stylesheet" href="assets/messenger/build/css/messenger-theme-future.css">
    <link rel="stylesheet" href="assets/vendor/zocial.css">
    <link rel="stylesheet" href="assets/vendor/animate-custom.css">
    <link rel="stylesheet" href="assets/main.css">
    <script src="assets/modernizr/modernizr.js"></script>
    <script type="text/javascript" src="http://maps.google.com/maps/api/js?sensor=false"></script>
    
  </head>
  <body>
    <!--[if lt IE 7]>   
        <p class="chromeframe">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade your browser</a> or <a href="http://www.google.com/chromeframe/?redirect=true">activate Google Chrome Frame</a> to improve your experience.</p>
    <![endif]-->    

    <div id="container">

      <div class="row">
        <!-- LEFT COLUMN -->
        <div class="col-md-2 left_top_column">
          <article class="top_nav">
            <div id="grafmap_logo" class="logo animated slideInLeft">
              <label>GrafMap</label>              
            </div>
            <div class="button_login">
              <button id="fb_button_login" class='zocial facebook'>Login</button>
            </div>
            <section id="profile_user">
              <img src="" class="img-thumbnail img-responsive">
              <label class="name"></label>
            </section>           
            <!-- <fb:login-button show-faces="true" width="200" max-rows="1"></fb:login-button> -->
          </article>          
        </div>
        <!-- RIGHT COLUMN -->
        <div class="col-md-10">
          <div id="map"></div>
        </div>        
      </div>
    </div>    
    <!-- Handlabars templates  -->

    <script id="info-window-template" type="text/x-handlebars-template">
        <div class="info-window">
          <div class="content-frame">
            <div class="media">
              <div class="pull-left">
                <img class="media-object" alt="64x64" style="width: 64px; height: 64px;" src="{{picture.data.url}}">
                <div class="social-buttons">
                  <a href="{{link}}" target='_blank' class="fa-stack fa-lg">
                    <i class="fa fa-square-o fa-stack-2x"></i>
                    <i class="fa fa-facebook fa-stack-1x"></i>
                  </a>
                </div>
              </div>
              <div class="media-body">
                <h4 class="media-heading">{{name}}</h4>
                {{truncate description 150}}
              </div>
              <footer>
                <button data-place-id="{{id}}" data-latitude="{{location.latitude}}" data-longitude="{{location.longitude}}" type="button" class="btn btn-default favorite_button {{favoritedClass id}}" {{isDisable id}}>{{{favoritedText id}}}</button>
              </footer>              
            </div>          
          </div>
        </div>
    </script>


    <script src="assets/jquery/jquery.js"></script>
    <!-- build:js scripts/plugins.js -->
    <script src="assets/handlebars/handlebars.min.js"></script>
    <script src="assets/bootstrap-sass/js/affix.js"></script>
    <script src="assets/bootstrap-sass/js/alert.js"></script>
    <script src="assets/bootstrap-sass/js/dropdown.js"></script>
    <script src="assets/bootstrap-sass/js/tooltip.js"></script>
    <script src="assets/bootstrap-sass/js/modal.js"></script>
    <script src="assets/bootstrap-sass/js/transition.js"></script>
    <script src="assets/bootstrap-sass/js/button.js"></script>
    <script src="assets/bootstrap-sass/js/popover.js"></script>
    <script src="assets/bootstrap-sass/js/carousel.js"></script>
    <script src="assets/bootstrap-sass/js/scrollspy.js"></script>
    <script src="assets/bootstrap-sass/js/collapse.js"></script>
    <script src="assets/bootstrap-sass/js/tab.js"></script>
    <script src="assets/messenger/build/js/messenger.min.js"></script>
    <script src="assets/messenger/build/js/messenger-theme-future.js"></script>    
    <!-- endbuild -->
    <script src="assets/vendor/fontawesome-markers.js"></script>    
    <script src="assets/main.js"></script>
  </body>
</html>
